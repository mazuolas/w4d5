<!DOCTYPE html>
<html>
  <head>
    <title>GoalSetter</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if current_user %>
      <%= button_to 'Log out', sessions_url, method: 'delete' %>
      
    <% end %>
    <%= yield %>
  </body>
</html>
